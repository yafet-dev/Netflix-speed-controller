{
  "manifest_version": 3,
  "name": "Netflix Speed Controller",
  "version": "1.0",
  "description": "Control playback speed on Netflix",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.netflix.com/*"],
      "js": ["content.js"]
    }
  ]
}
